<?xml version="1.0" encoding="UTF-8"?><components xmlns:scr="http://www.osgi.org/xmlns/scr/v1.0.0">
    <scr:component enabled="true" immediate="true" name="com.cqblueprints.example.services.GoodbyeWorldService">
        <implementation class="com.cqblueprints.example.services.GoodbyeWorldService"/>
        <service servicefactory="false">
            <provide interface="com.cqblueprints.example.services.GoodbyeWorldService"/>
        </service>
        <property name="service.vendor" type="String" value="CQ Blueprints"/>
        <property name="service.description" type="String" value="Provides a friendly farewell."/>
        <property name="service.pid" value="com.cqblueprints.example.services.GoodbyeWorldService"/>
    </scr:component>
    <scr:component enabled="true" name="com.cqblueprints.example.services.MySmpleWorkflow">
        <implementation class="com.cqblueprints.example.services.MySmpleWorkflow"/>
        <service servicefactory="false">
            <provide interface="com.day.cq.workflow.exec.WorkflowProcess"/>
        </service>
        <property name="service.description" type="String" value="Fetch Dependencies of node."/>
        <property name="service.vendor" type="String" value="Adobe"/>
        <property name="process.label" type="String" value="Fetch Nodes"/>
        <property name="service.pid" value="com.cqblueprints.example.services.MySmpleWorkflow"/>
        <reference name="repository" interface="org.apache.sling.jcr.api.SlingRepository" cardinality="1..1" policy="static" bind="bindRepository" unbind="unbindRepository"/>
        <reference name="queryBuilder" interface="com.day.cq.search.QueryBuilder" cardinality="1..1" policy="static" bind="bindQueryBuilder" unbind="unbindQueryBuilder"/>
    </scr:component>
    <scr:component enabled="true" name="com.cqblueprints.example.services.SampleServlet">
        <implementation class="com.cqblueprints.example.services.SampleServlet"/>
        <service servicefactory="false">
            <provide interface="javax.servlet.Servlet"/>
        </service>
        <property name="sling.servlet.paths" value="/bin/service/testnew"/>
        <property name="sling.servlet.methods" value="GET"/>
        <property name="service.pid" value="com.cqblueprints.example.services.SampleServlet"/>
        <reference name="repository" interface="org.apache.sling.jcr.api.SlingRepository" cardinality="1..1" policy="static" bind="bindRepository" unbind="unbindRepository"/>
    </scr:component>
</components>
